{
  "name": "preact-charts",
  "version": "0.4.1",
  "description": "Charting library built using preact with some small d3 dependencies",
  "main": "dist/preactCharts.js",
  "umd:main": "dist/preactCharts.umd.js",
  "module": "dist/preactCharts.mjs",
  "source": "src/preactCharts.ts",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "typecheck": "tsc --noEmit",
    "smart-publish": "npm run build && git commit -am %npm_package_version% && git tag %npm_package_version% && git push && git push --tags && npm publish"
  },
  "keywords": [
    "charts",
    "data-vis",
    "preact",
    "d3"
  ],
  "author": "pmkroeker <dev.pmkro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmkroeker/preact-charts.git"
  },
  "bugs": {
    "url": "https://github.com/pmkroeker/preact-charts/issues"
  },
  "dependencies": {
    "d3-array": "^2.0.3",
    "d3-axis": "^1.0.12",
    "d3-brush": "^1.0.6",
    "d3-scale": "^2.2.2",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "d3-time": "^1.0.11",
    "d3-time-format": "^2.1.3",
    "typestyle": "^2.0.1"
  },
  "peerDependencies": {
    "preact": "^8.4.2",
    "@types/d3": "^5.7.1"
  },
  "devDependencies": {
    "@types/d3": "^5.7.1",
    "microbundle": "^0.10.1",
    "preact": "^8.4.2",
    "rollup": "^1.0.0",
    "tslint": "^5.12.0",
    "typescript": "^3.3.3"
  }
}
